<UserControl x:Class="Prototype4.MapControll"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Prototype4"
             mc:Ignorable="d"
             d:DesignHeight="731" d:DesignWidth="800">
    <Grid x:Name="Border">
        <Viewbox x:Name="ViewBox" 
            Stretch="None" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Image x:Name="mapElement" ClipToBounds="True" HorizontalAlignment="Center"
               VerticalAlignment="Center" Source="res/Test.jpg" Stretch="None" >
            </Image>
        </Viewbox>
    </Grid>
</UserControl>
