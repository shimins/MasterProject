<Window x:Class="basic.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:basic"
        mc:Ignorable="d"
        Title="MainWindow" Height="1000" Width="1800" WindowStartupLocation="CenterScreen">
    <Grid x:Name="Grid">
        <Viewbox x:Name="ViewBox"  MouseLeftButtonDown="Rectangle_OnMouseLeftButtonDown"  MouseLeftButtonUp="Rectangle_OnMouseLeftButtonUp"
                   MouseMove="Rectangle_OnMouseMove" MouseWheel="Rectangle_OnMouseWheel"
            Stretch="None" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Image x:Name="image" ClipToBounds="True" HorizontalAlignment="Center"
               VerticalAlignment="Center" Source="res/Test.jpg" Height="8192" Width="8192">
            </Image>
        </Viewbox>
        <!--<Rectangle Name="Rectangle" Fill="Yellow" HorizontalAlignment="Center" Height="279" Stroke="Black" VerticalAlignment="Center" Width="381" 
                   MouseLeftButtonDown="Rectangle_OnMouseLeftButtonDown"  MouseLeftButtonUp="Rectangle_OnMouseLeftButtonUp"
                   MouseMove="Rectangle_OnMouseMove" MouseWheel="Rectangle_OnMouseWheel" />-->
    </Grid>
</Window>
